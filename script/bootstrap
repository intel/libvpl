# Build base dependencies.

set -o errexit


SCRIPT_DIR="$( cd "$(dirname "${BASH_SOURCE[0]:-$0}")" >/dev/null 2>&1 ; pwd -P )"

# Read command line options
. "${SCRIPT_DIR}/_buildopts.sh" \
    --name bootstrap \
    --desc "Build base dependencies." \
    -- "$@"

pushd "${PROJ_DIR}"
    echo "No dependencies for base."
popd